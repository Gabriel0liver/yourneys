<main class="container">
  <section id="layout-profile">
    <div class="profile-details">
      <div class="pic">
        <img src="{{user.profilepic}}" alt="profile-pic" class="profile-pic">
      </div>
      <h2>{{user.username}}</h2>
    </div>
    <h3>About Me</h3>
    <p>{{user.description}}</p>

    <h3>Created Yourneys</h3>
    <ul>
      {{#each createdYourneys}}
      {{> yourneycard this}}
      {{/each}}
    </ul>
  </section>
</main>